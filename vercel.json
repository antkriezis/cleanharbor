{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload.py"
    }
  ],
  "functions": {
    "api/upload.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}

